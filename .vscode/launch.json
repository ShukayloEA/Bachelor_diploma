    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}\\NanoTrack\\", 
            "justMyCode": false,
            "args": [
                "--vis",
            ]
        }
    ]

    //args: "--img", "640", "--batch", "4", "--epochs", "10", "--data", "data_visible.yaml", "--weights", "yolov5s.pt", "--device", "cpu"
}